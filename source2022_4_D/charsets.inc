
// Some Useful Sets of Characters

CONST
// All whitespace characters
 PWU_WHITESPACE : Set Of Char = [#7, #8, #9, #10, #12, #13, #32];
// Text End Of Line Characters 
 PWU_ENDLINE : Set of Char = [#10, #13];
 
 PWU_DIGITS : Set Of Char = [ '1','2','3','4','5','6','7','8','9','0'];
 PWU_LOWERCASE : Set Of Char = ['a'..'z'];
 PWU_UPPERCASE : Set Of Char = ['A'..'Z'];
